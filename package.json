{
  "name": "@skyleague/event-horizon-dev",
  "version": "1.0.0",
  "description": "The accompanying development package for @skyleague/event-horizon.",
  "license": "MIT",
  "author": "SkyLeague Technologies B.V. <technologies@skyleague.io> (https://skyleague.io)",
  "type": "module",
  "exports": {
    ".": "./dist/index.js",
    "./package.json": "./package.json",
    "./*.js": "./dist/*.js"
  },
  "types": "./dist/index.d.ts",
  "bin": {
    "event-horizon": "./bin/run.js",
    "event-horizon-dev": "./bin/run.js"
  },
  "files": [
    "bin",
    "dist",
    "package.json"
  ],
  "scripts": {
    "build": "npx ts-node --esm build.config.ts",
    "build:clean": "npx tsc --build --clean && rm -rf dist",
    "build:docs": "npx typedoc",
    "check:cost": "npx cost-of-modules --no-install --include-dev",
    "check:coverage": "NODE_OPTIONS=\"$NODE_OPTIONS --experimental-vm-modules\" npx jest --collectCoverage=true",
    "check:full": "npm run lint && npm run check:types && npm run check:coverage && npm run build && npm run check:project",
    "check:project": "npx node-standards lint",
    "check:types": "npx tsc -p tsconfig.json",
    "format": "npx prettier \"**/*.{ts,js,json,yml,yaml,md}\" --write",
    "lint": "npx eslint \"./{src,test,typings}/**/*.ts\" --cache --no-eslintrc --config .eslintrc.cjs",
    "lint:fix": "npm run lint -- --fix",
    "lint:full": "bash -c \"FULL_LINT=true npm run lint\"",
    "package": "rm -rf .dist && npm run build",
    "prepare": "npx husky install || true",
    "test": "NODE_OPTIONS=\"$NODE_OPTIONS --experimental-vm-modules\" npx jest"
  },
  "dependencies": {
    "@aws-sdk/client-appconfig": "^3.303.0",
    "@aws-sdk/client-appconfigdata": "^3.303.0",
    "@aws-sdk/client-dynamodb": "^3.303.0",
    "@aws-sdk/client-eventbridge": "^3.303.0",
    "@aws-sdk/client-firehose": "^3.303.0",
    "@aws-sdk/client-s3": "^3.303.0",
    "@aws-sdk/client-secrets-manager": "^3.303.0",
    "@aws-sdk/client-sfn": "^3.303.0",
    "@aws-sdk/client-sqs": "^3.303.0",
    "@aws-sdk/client-ssm": "^3.303.0",
    "@aws-sdk/client-ssm-incidents": "^3.303.0",
    "@aws-sdk/lib-dynamodb": "^3.303.0",
    "@aws-sdk/lib-storage": "^3.303.0",
    "@skyleague/axioms": "^2.0.0",
    "@skyleague/therefore": "^2.0.0",
    "@types/express": "^4.17.17",
    "express": "^4.18.2",
    "express-list-endpoints": "^6.0.0",
    "express-promise-router": "^4.1.1",
    "source-map-support": "^0.5.21",
    "tslib": "^2.5.0",
    "yargs": "^17.7.1"
  },
  "devDependencies": {
    "@skyleague/event-horizon": "^4.0.1",
    "@skyleague/node-standards": "^2.2.0",
    "@types/aws-lambda": "^8.10.114",
    "@types/source-map-support": "^0.5.6",
    "@types/yargs": "^17.0.24",
    "typescript": "^5.0.2"
  },
  "peerDependencies": {
    "@skyleague/event-horizon": "^4.0.0-beta.0"
  },
  "engines": {
    "node": ">=18"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  },
  "node-standards": {
    "extends": "yargs-cli"
  }
}
